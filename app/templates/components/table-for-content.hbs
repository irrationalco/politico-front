{{yield}}
<div class="content" style="overflow: auto;">
	<table class="table table-striped table-hover table-condensed">
		<thead class="voter-table-header">
			<tr>
				<td>Clave de Elector</td>
				<td>Nombre</td>
				<td>Paterno</td>
				<td>Materno</td>
				<td>Estado</td>
				<td></td>
			</tr>
		</thead>

		<tbody class="voter-table-content">
			{{#each model as |voter|}}
				{{voter-row voter=voter deleteAction=deleteAction}}
			{{/each}}
		</tbody>
	</table>
</div>
